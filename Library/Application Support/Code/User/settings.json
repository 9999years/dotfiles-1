{
    "editor.fontFamily": "'Iosevka SS08', Iosevka, Menlo, Monaco, 'Courier New', monospace",
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 80,
    "editor.renderWhitespace": "all",
    "workbench.colorTheme": "Material Theme Italicize",
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontLigatures": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": false,
    "files.trimTrailingWhitespace": true,
    "terminal.external.osxExec": "iTerm 2.app",
    "files.associations": {
        "Dockerfile*": "dockerfile",
        "*.sc": "scala"
    },
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.historyExplorer.enabled": true,
    "gitlens.codeLens.scopes": [
        "document",
        // "blocks",
        // "containers"
    ],
    "rust.all_targets": true,
    // "rust.unstable_features": true,
    "rust-client.logToFile": false,
    "rust-client.revealOutputChannelOn": "warn",
    "rust-client.updateOnStartup": true,
    "rust.cfg_test": true,
    "rust.goto_def_racer_fallback": true,
    "window.zoomLevel": 0,
    "terminal.integrated.rendererType": "dom",
    "git.confirmSync": false,
    "materialTheme.fixIconsRunning": false,
    "breadcrumbs.enabled": true,
    "terminal.integrated.fontFamily": "'Iosevka SS08', Iosevka",
    "terminal.integrated.fontSize": 13,
    "workbench.fontAliasing": "auto",
    "git.enableSmartCommit": true,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "git.autofetch": true,
    "editor.fontSize": 13,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "rust.clippy_preference": "on",
    "editor.formatOnSave": true,
}
